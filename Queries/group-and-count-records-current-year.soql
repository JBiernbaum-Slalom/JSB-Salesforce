SELECT Field1, Field2, Field3, Field4, COUNT(Id) recordCount
FROM ObjectName
WHERE CreatedDate = THIS_YEAR
GROUP BY Field1, Field2, Field3, Field4
ORDER BY Field1, Field2, Field3, Field4

